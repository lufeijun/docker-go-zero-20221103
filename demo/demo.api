type Request {
}

type Response {
	Status  int8        `json:"status"`
	Message string      `json:"message"`
	Values  interface{} `json:"values"`
}

service demo-api {
	@handler Hello
	get /hello returns (Response)
	
	@handler Version
	get /version returns (Response)
	
	@handler Delay
	get /delay returns (Response)
	
}